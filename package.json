{
  "name": "gpea-hongkong-pet-parcel_story",
  "version": "1.0.0",
  "description": "a boilerplate with parcel and storybook",
  "scripts": {
    "start": "parcel serve src/index.html",
    "parcel": "parcel serve",
    "deploy": "yarn build && gh-pages -d dist",
    "build": "yarn clean && parcel build src/*.html --public-url ./ --no-minify",
    "build:en": "yarn clean && parcel build ./src/*.html --public-url https://api.greenpeace.org.hk/2020/cooking/ --no-minify",
    "clean": "rm -rf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aos": "^2.3.4",
    "flexboxgrid": "^6.3.1",
    "jquery": "^3.5.0",
    "normalize.css": "^8.0.1",
    "polyfill": "^0.1.0",
    "remove-focus-outline": "^1.2.1",
    "smooth-scroll": "^16.1.3",
    "swiper": "^5.3.8",
    "tailwindcss": "^1.2.0",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "parcel": "latest",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-bundle-visualiser": "^1.2.0",
    "postcss": "^7.0.27",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.5.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^2.0.1",
    "sass": "^1.26.3",
    "storybook-mobile": "^0.1.16"
  }
}
